    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clipboard": "^2.0.11",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookie": "^1.0.2",
    "csstype": "^3.1.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
