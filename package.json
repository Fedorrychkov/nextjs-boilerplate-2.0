    "react-hook-form": "^7.61.1",
    "react-json-pretty": "^2.2.0",
    "react-query": "^3.39.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.9.3",
